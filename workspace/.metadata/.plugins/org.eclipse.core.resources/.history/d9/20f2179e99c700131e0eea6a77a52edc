import java.util.*;
class team
{
	String name;
	protected int wins;
	protected int losses;
	team(String name, int wins, int losses)
	{
		this.name = name;
		this.wins = wins;
		this.losses = losses;
	}
}
class basketball extends team
{
	int pointsfor;
	int pointsagainst;
	double winloss;
	
	void calcwinloss(int wins, int losses)
	{
		this.wins = wins;
		this.losses = losses;
		winloss = (double)(wins/(wins+losses));
	}
	double getlosses()
	{
		return this.winloss;
	}

	basketball(String name, int wins, int losses) 
	{
		super(name, wins, losses);
		this.wins = wins;
		this.losses = losses;
		this.name = name;
		calcwinloss(wins, losses);
	}
	
}

public class finalpractice 
{

	public static void main(String[] args) 
	{
		int num = 1;
		switch(num)
		{
		case(1):
		{
			System.out.println("got it");
		}
		break;
		case(5):
		{
			System.out.println("bang!");
		}
		break;
		}
	}

}
